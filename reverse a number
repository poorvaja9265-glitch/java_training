import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n;
        n=s.nextInt();
        int m=0;
        while(n!=0)
            
        {
            int d=n%10;
            m=m*10+d;
            n=n/10;
        }
        System.out.println(m);
        
    }
}
