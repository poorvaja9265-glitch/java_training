import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner S =new Scanner(System.in);
        int N;
        N=S.nextInt();
        if(N%2==0)
        {
            System.out.println("Even");
        }
        else
        {
           System.out.println("Odd"); 
        }
       
    }
}
