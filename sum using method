import java.io.*;
import java.util.*;

public class Solution {
    public int show(int a,int b)
    {
        int c=a+b;
        return c;
    }

    public static void main(String[] args) {
        Scanner s =new Scanner(System.in);
        int a=s.nextInt();
        int b=s.nextInt();
        Solution obj=new Solution();
        int sum=obj.show(a,b);
        System.out.println(sum);
    }
}
