import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s =new Scanner(System.in);
    int a=s.nextInt();
       int b=s.nextInt();
        int sum=a+b;
        int diff=a-b;
        int mul=a*b;
        int product=a/b;
        System.out.println(sum);
        System.out.println(diff);
        System.out.println(mul);
        System.out.println(product);
        
        }
}
