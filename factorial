import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n;
        long f=1;
        n=s.nextInt();
        for(int i=1;i<=n;i++)
        {
            f=f*i;
        }
            
            System.out.println(f);
        
    }
}
