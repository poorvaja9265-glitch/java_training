import java.io.*;
import java.util.*;

public class Solution {
        double total;
        Solution(double balance,double deposite)
        {
        total=balance+deposite;        
        }
        void display()
        {
                System.out.println("Final balance:"+total);
                 
        }
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        String name=s.nextLine();
        double balance=s.nextDouble();
        double deposite=s.nextDouble();
        Solution s2= new Solution(balance,deposite);
            s2.display();
    }
}

