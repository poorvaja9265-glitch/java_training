import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n;
        n=s.nextInt();
        if(n%1==0)
        {
            if(n%n==0)
            {
                System.out.println("Prime");
            }
            else
            {
               System.out.println(" not Prime");
            }
        }
    }
}

