import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n;
        n=s.nextInt();
        for(int i=1;i<=n;i++)
        {
            if(i%2==0){
              System.out.print(i+" ");
            }
            
        }
    }
}

